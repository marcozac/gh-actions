{
    "name": "@marcozac/gh-actions",
    "version": "0.0.0",
    "description": "A collection of github actions",
    "type": "module",
    "scripts": {
        "build": "rollup --config rollup.config.js",
        "format": "prettier --write --ignore-unknown 'src/**/*.{js,ts,json}'",
        "lint": "eslint src/**/*.ts",
        "lint-staged": "lint-staged --quiet",
        "prepare": "husky install",
        "test": "jest",
        "typecheck": "tsc"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/marcozac/gh-actions.git"
    },
    "keywords": [
        "actions",
        "node",
        "setup"
    ],
    "author": "marcozac <marco@marcozac.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/marcozac/gh-actions/issues"
    },
    "homepage": "https://github.com/marcozac/gh-actions#readme",
    "dependencies": {
        "@actions/core": "^1.7.0"
    },
    "devDependencies": {
        "@jest/types": "^28.0.2",
        "@marcozac/eslint-config": "^0.3.0",
        "@rollup/plugin-commonjs": "^22.0.0",
        "@rollup/plugin-node-resolve": "^13.3.0",
        "@rollup/plugin-typescript": "^8.3.2",
        "@types/jest": "^27.5.0",
        "@types/node": "^17.0.31",
        "@types/prettier": "^2.6.0",
        "eslint": "^8.14.0",
        "husky": "^7.0.0",
        "jest": "^28.0.3",
        "lint-staged": "^12.4.1",
        "prettier": "2.6.2",
        "rollup": "^2.71.1",
        "ts-jest": "^28.0.0",
        "tslib": "^2.4.0",
        "typescript": "^4.6.4"
    }
}
